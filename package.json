{
  "name": "api-express-backend",
  "version": "1.0.0",
  "description": "API Node & Express - BACK-END 1ยบ Semestre de 2024 IFSP",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --env-file=.env --watch src/server.js",
    "prisma:pull": "npx prisma db pull --schema=./prisma/schema-prod.prisma",
    "prisma:push": "npx prisma db push --schema=./prisma/schema-prod.prisma",
    "prisma:generate": "npx prisma generate --schema=./prisma/schema-prod.prisma",
    "prisma:local:pull": "npx prisma db pull --schema=./prisma/schema-local.prisma",
    "prisma:local:push": "npx prisma db push --schema=./prisma/schema-local.prisma",
    "prisma:local:generate": "npx prisma generate --schema=./prisma/schema-local.prisma",
    "prisma:local:studio": "npx prisma studio --schema=./prisma/schema-local.prisma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Guibs27/api-express-backend.git"
  },
  "keywords": [
    "api",
    "express",
    "zod",
    "mvc"
  ],
  "author": "Guilherme Byz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Guibs27/api-express-backend/issues"
  },
  "homepage": "https://github.com/Guibs27/api-express-backend#readme",
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^11.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "prisma": "^5.20.0"
  }
}
